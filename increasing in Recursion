public class PrintNumbersRecursion {

    public static void inrecursion(int n) {
        if (n == 1) {
            System.out.print(n + " ");
            return;
        }
        inrecursion(n - 1);
        System.out.print(n + " ");
    }

    public static void main(String[] args) {
        int n = 5;
        inrecursion(n);
    }
}
