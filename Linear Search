public class Array {
   public static int linears(int arr[],int key){
        for(int i=0;i<arr.length;i++){
            if(arr[i]==key){
                return i;
            }
         
        }

                return -1;
                  
    }
  public static void main(String[] args) {
    int arr[]={1,2,5,3,4};
    System.out.println(linears(arr,3));
  }
}
