public class DecreasingRecursion {
 public static void decrecursion(int n) {
        if (n == 1) {
            System.out.print(n);
            return;
        }
        System.out.print(n + " ");
        decrecursion(n - 1);
    }
    public static void main(String[] args) {
        int n = 5;
        decrecursion(n);
    }
}
