public class Array {
   public static int minele(int arr[]){
    int min=arr[0];
    for(int i=0;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
    }
    return min;
}

    public static void main(String[] args) {
        int arr[]={3,9,8,1,4,10};
        System.out.println(minele(arr));
    }
    
}
