public class Array {
   public static int maxele(int arr[]){
    int max=arr[0];
    for(int i=0;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
            }
    }
    return max;
}

    public static void main(String[] args) {
        int arr[]={3,9,8,6,4,10};
        System.out.println(maxele(arr));
    }
    
}
